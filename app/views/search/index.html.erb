<h3>Stations Near You</h3>
<div class="container">
  <% @stations.each do |station| %>
    <h6>Name: <%= station.name %></h6>
    <li>Address: <%= station.address %></li>
    <li>Fuel Type: <%= station.fuel_type %></li>
    <li>Distance: <%= station.distance %></li>
    <li>Access Times <%= station.access_time %></li>
  <% end %>
</div>
